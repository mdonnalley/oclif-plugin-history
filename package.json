{
  "name": "@oclif/plugin-history",
  "version": "0.0.0",
  "description": "plugin for exploring history of a CLI",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/oclif/plugin-history",
  "bugs": "https://github.com/oclif/plugin-history/issues",
  "repository": "oclif/plugin-history",
  "license": "MIT",
  "author": "Salesforce",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "oex": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "yarn lint",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "oex",
    "commands": "./dist/commands",
    "dirname": "oex",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "dependencies": {
    "@inkjs/ui": "^1.0.0",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "ansis": "^3.2.0",
    "ink": "^4.4.1",
    "react": "^18.3.1",
    "semver": "^7.6.1",
    "terminal-link": "^3.0.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/react": "^18.3.1",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-xo": "^0.45.0",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "mocha": "^10",
    "oclif": "^4.9.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "xo": "^0.58.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
